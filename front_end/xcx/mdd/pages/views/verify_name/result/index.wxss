
.header {
  background-color: #2954f9;
  color: #fff;
  padding: 10rpx 0 30rpx 40rpx;
}
.li {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 30rpx;
  border-bottom: 1px solid #ddd;
  background-color: #fff;
}
.li .strong {
  font-weight: 900;
  margin: 0 10rpx;
}
.li .check {
  color: #2954f9;
  font-size: 26rpx;
}
